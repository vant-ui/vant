{
  "name": {
    "title": "折叠面板容器",
    "description": "组件名称"
  },
  "pkgId": {
    "title": "antd",
    "description": "组件库包名称"
  },
  "props": {
    "type": "object",
    "title": "组件属性",
    "required": ["types"],
    "properties": {
      "accordion": {
        "type": "boolean",
        "title": "手风琴模式",
        "valueType": "switch"
      },
      "activeKey": {
        "type": ["string", "number", "array"],
        "title": "展开项",
        "valueType": "text"
      },
      "expandIcon": {
        "type": "string",
        "title": "展开图标",
        "valueType": "text"
      },
      "defaultActiveKey": {
        "type": ["string", "number", "array"],
        "title": "初始展开项",
        "valueType": "text"
      }
    },
    "description": "组件属性"
  },
  "events": {
    "type": "object",
    "title": "交互行为",
    "required": [],
    "properties": {
      "onChange": {
        "type": "object",
        "title": "状态改变时",
        "properties": {
          "args": {
            "type": "array",
            "title": "参数列表",
            "valueType": "tags"
          },
          "name": {
            "type": "string",
            "title": "函数名"
          }
        }
      }
    },
    "description": "组件动作事件"
  },
  "tagName": {
    "title": "Accordion",
    "description": "组件标签"
  },
  "version": {
    "title": "1.0",
    "description": "组件库包版本"
  },
  "children": {
    "type": "array",
    "title": "子组件",
    "description": "嵌套进的子组件的json object(此处例子,只有hasChild为true时才存在)"
  },
  "hasChild": {
    "type": "boolean",
    "const": false,
    "title": "支持调用子组件",
    "description": "true:支持;false:不支持"
  },
  "protoProps": {
    "type": "object",
    "title": "原生属性",
    "required": [],
    "properties": {
      "id": {
        "type": "string",
        "title": "唯一id",
        "valueType": "text",
        "description": "元素唯一标识"
      },
      "ref": {
        "type": "string",
        "title": "ref",
        "valueType": "text",
        "description": "组件ref名称，可在this.[ref名称]来获取组件实例this对象"
      },
      "style": {
        "type": "object",
        "title": "内联样式",
        "valueType": "object",
        "description": "组件内联样式(JSON格式)"
      },
      "className": {
        "type": "string",
        "title": "样式名",
        "valueType": "textarea",
        "description": "组件样式名"
      }
    },
    "description": "原生属性"
  },
  "lastModifyTime": {
    "type": "string",
    "title": "最后一次修改时间",
    "description": "格式为yyyy-mm-dd"
  }
}
